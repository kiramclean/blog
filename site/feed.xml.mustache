---
site/leave-path-alone: true
site/no-layout: true
---
<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>Kira McLean</title>
    <link>{{meta/root-url}}</link>
    <description>Kira's thoughts and guides on tech, life, and meaning.</description>
    <lastBuildDate>{{meta/last-modified}}</lastBuildDate>
    <atom:link href="{{meta/root-url}}/feed.xml" rel="self" type="application/rss+xml"/>
    <generator>Dynamo v0.0</generator>
    <docs>https://validator.w3.org/feed/docs/rss2.html</docs>
      {{#blog}}
      <item>
        <title>{{title}}</title>
        <description>{{content}}</description>
        <pubDate>{{rss-date}}</pubDate>
        <link>{{meta/root-url}}{{slug}}</link>
        <guid isPermaLink="true">{{meta/root-url}}{{slug}}</guid>
      </item>
    {{/blog}}
  </channel>
</rss>

